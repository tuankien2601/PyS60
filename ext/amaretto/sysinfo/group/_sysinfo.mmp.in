targettype      dll
TARGET	        ${{PREFIX}}_sysinfo.pyd

#ifdef EKA2
CAPABILITY ${{DLL_CAPABILITIES}}
#endif

${{COMPILER_FLAGS}}
NOSTRICTDEF

SYSTEMINCLUDE    \epoc32\include\python25
SYSTEMINCLUDE    \epoc32\include\stdapis
SYSTEMINCLUDE    \epoc32\include

USERINCLUDE   ..\inc

LIBRARY python25.lib
LIBRARY euser.lib
LIBRARY sysutil.lib
${{if S60_VERSION<30
LIBRARY plpvariant.lib
LIBRARY sysagt.lib
}}
LIBRARY hal.lib
LIBRARY efsrv.lib 
${{if S60_VERSION>12
LIBRARY platformenv.lib
}}
${{if S60_VERSION>26
LIBRARY etel3rdparty.lib
}}
#ifdef EKA2
LIBRARY centralrepository.lib 
#endif

SOURCEPATH    ..\src

source		sysinfomodule.cpp

${{if S60_VERSION>26
source asynccallhandler.cpp
}}


#if defined(ARMCC)
deffile eabi\_sysinfo 
#elif defined(WINSCW)
deffile bwins\_sysinfo 
#endif
